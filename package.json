{
  "name": "jeopardy-ai",
  "version": "1.0.0",
  "description": "AI-powered Jeopardy game with multi-game support",
  "scripts": {
    "install": "cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "uvicorn app.main:app --host 0.0.0.0 --port ${PORT:-8000} --ws wsproto",
    "dev": "concurrently \"uvicorn app.main:app --reload --ws wsproto\" \"cd frontend && npm run dev\"",
    "dev:backend": "uvicorn app.main:app --reload --ws wsproto",
    "dev:frontend": "cd frontend && npm run dev"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/jeopardy-ai.git"
  },
  "keywords": [
    "jeopardy",
    "ai",
    "game",
    "trivia",
    "websocket"
  ],
  "author": "",
  "license": "MIT"
}
